/**********************/
/* Global Definitions */

body {
   margin: 0px;
   padding-top: 0px;
}

a {
 border: 0;  
}

img {
  border: 0;
}

table {
  border: 0;
  border-spacing:0;
   padding:0px;
}

div {
  border: 0;
  border-spacing:0;
}

tr {
  font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: 10px;
    color: #447385;
    text-align: left;
    padding:0px;
}

td {
  font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: 10px;
    color: #447385;
    text-align: left;
}

th {
   font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: 10px;
    color: #447385;
    text-align: left;
}



/****************************/
/* App Specific Definitions */

.clientMainDiv {
   position:absolute;
   top:5%; 
   left:22%;
   border:thin solid;
   width:75%; 
   height:80%;
}

.clientMainColumn {
   padding:0px; 
}

.clientLeftDiv {
   position:absolute;
   top:5%; 
   left:5%;
   border:thin solid;
   width:13%; 
   height:80%;
}

.panelBar {
   width:100%;
   height:10px;
   border-collapse: separate;
   border-spacing: 0;
   border: 0;
   background-color: #447385;
}

.troubleshooterPanelTable {
   width:100%;
   height:100%;
   border-collapse: separate;
   border-spacing: 0;
   border: 0;
}

.troubleshooterLeftPanel {
   width:100%;
   border-collapse: separate;
   border-spacing: 0;
   border: 0;
}

.troubleshooterLeftPanelTable {
   width:100%;
   height:100%;
   vertical-align: top;
   border-collapse: separate;
   border-spacing: 0;
   border: 0;
}

.troubleshooterLeftTableRow {
   vertical-align: top;
   width:100%;
   height:100%;
}

.connectionTable {
   vertical-align: top;
   width:100%;
   height:100%;
}

.connectionHead {
   vertical-align: top;
   width:100%;
   height:5%;
}

.connectionTopRow {
   vertical-align: top;
   width:100%;
   height:20%;
}

.connectionAddBox {
   vertical-align: top;
   width:100%;
   height:10%;
}

.connectionFormBox {
   vertical-align: top;
   visibility: hidden; 
   height: 0px; 
   width: 0px;
}

.connectionField {     
     font-size: 11px;
     width: 100px;
     margin-right: 6px;
}

.troubleshooterTableMenuRow {
   width:100%;
   height:5%;
}

.troubleshooterTableHeadRow {
   width:100%;
   height:2%;
}

.troubleshooterTableBodyRow {
   width:100%;
   height:15%;
}

.troubleshooterTableDividerRow {
   width:100%;
   height:1%;
}

.troubleshooterTableRow {
   width:100%;
}

.troubleshooterContentTableRow {
   width:100%;
   height:100%;
}

.troubleshooterContentTableColumn {
   width:100%;
   height:100%;
}

.menuBarIFrame {
    top:0px; 
    height:100%;
    width:100%
}

.connectionPanelIFrame {
    vertical-align: top; 
    height:400px;
    width:100%;
}

.headIFrame {
    width:100%;
    height:15px;
}

.bodyIFrame {
    height:100%;
    width:100%;
}

.contentIFrame {
    height:100%;
    width:100%;
}

.dividingDiv {
   border:thin solid;
   width:100%; 
   height:1px;
}

.dividingDivCol {
   height:1px;
}

.troubleshooterTableDividerRow {
   height:1px;
}

.headDiv {
   width:100%; 
   height:15px;
}

.menuTableHead {
  border-collapse: collapse;
  border-spacing: 0;
  border: 0;
  width:100%;
}

.menuTablePanel {
   position:absolute;
   border: 0;
   border-spacing: 0;
  
}
.operationTableHead {
     border-collapse: collapse;
     border-spacing: 0;
     width:100%;
     height:100%;

}

.operationTableBody {
     border-collapse: collapse;
     border-spacing: 0;
     width:100%;
     height:100%;
}

.operationTableRow {
     border-collapse: collapse;
   border-spacing: 0;
  width:100%;
   height:100%;
}


th.viewedHead {
    width: 100px;    
}

th.statusHead {
    width: 50px;
}

th.analysisHead {
    width: 150px;
}

th.operationNameHead {
    width: 350px;
}

th.executionsHead {
    width: 100px;
}

td.viewedColumn {
    width: 100px;    
}

td.statusColumn {
    width: 50px;
}

td.analysisColumn {
    width: 150px;
}

td.operationNameColumn {
    width: 350px;
}

td.executionsColumn {
    width: 100px;
}
