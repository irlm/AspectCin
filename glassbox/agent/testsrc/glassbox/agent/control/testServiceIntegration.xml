<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	  <bean id="testClient" class="glassbox.agent.control.ServiceIntegrationTest$TestClient">
		<property name="glassboxService"><ref bean="glassboxServiceProxy"/></property>
	  </bean>

	  <bean id="glassboxServiceProxy" class="org.springframework.jmx.access.MBeanProxyFactoryBean">
		<property name="proxyInterface" value="glassbox.agent.control.api.GlassboxService"/>
		<property name="serviceUrl" value="service:jmx:rmi://localhost:7232/jndi/rmi://localhost:7232/GlassboxTroubleshooter"/>
		<property name="objectName" value="Glassbox:type=service,name=default"/>
	  </bean>
	  
</beans>